{
  "name": "fullstackexample",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "db:migrate": "npx prisma migrate dev",
    "test:db:up": "docker compose -f docker-compose.test.yml up -d",
    "test:run": "jest",
    "test:db:down": "docker compose -f docker-compose.test.yml down",
    "test": "npm run test:db:up && npx dotenv-cli -e .env.test -- npm run db:migrate && npx dotenv-cli -e .env.test -- npm run test:run && npm run test:db:down",
    "dev-seed": "npx dotenv-cli -e .env.dev -- npx tsx src/dev.seed.ts",
    "run-express": "npx dotenv-cli -e .env.dev -- bun --watch src/server.ts"
  },
  "dependencies": {
    "@prisma/client": "^5.15.1",
    "@types/cors": "^2.8.17",
    "cors": "^2.8.5",
    "dotenv-cli": "^7.4.2",
    "express": "^4.19.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.23.1"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.12",
    "@types/react": "^18.2.66",
    "@types/react-dom": "^18.2.22",
    "@types/supertest": "^6.0.2",
    "@typescript-eslint/eslint-plugin": "^7.2.0",
    "@typescript-eslint/parser": "^7.2.0",
    "@vitejs/plugin-react": "^4.2.1",
    "eslint": "^8.57.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.6",
    "jest": "^29.7.0",
    "jest-mock-extended": "^2.0.4",
    "nodemon": "^3.1.3",
    "prisma": "^5.15.1",
    "supertest": "^7.0.0",
    "ts-jest": "^29.1.5",
    "ts-node": "^10.9.2",
    "typescript": "^5.4.5",
    "vite": "^5.2.0"
  }
}
